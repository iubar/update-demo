    "format-check": "npm run format -- --check",
    "clean-node": "rimraf node_modules"
  },
  "dependencies": {
    "expo": "~48.0.11",
    "expo-status-bar": "~1.4.4",
    "react": "18.2.0",
    "react-native": "0.71.6",
    "expo-updates": "~0.16.4"
  },
  "devDependencies": {
    "@babel/core": "^7.21.4",
    "@react-native-community/eslint-config": "^3.2.0",
    "babel-preset-expo": "^9.3.0",
    "eslint": "^8.38.0",
    "prettier": "^2.8.4",
    "react-test-renderer": "18.2.0",
    "rimraf": "^4.3.0"	
  },
  "private": true
}
